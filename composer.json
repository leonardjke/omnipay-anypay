{
    "name": "leonardjke/omnipay-anypay",
    "description": "Anypay gateway for Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "anypay",
    ],
    "homepage": "https://github.com/leonardjke/omnipay-anypay",
    "license": "MIT",
    "authors": [
        {
            "name": "leonardjke",
            "email": "a@leonardjke.net",
            "homepage": "https://github.com/leonardjke/omnipay-anypay"
        }
    ],
    "require": {
        "omnipay/common": "v3.0.5"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^1.3.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Anypay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Anypay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}
